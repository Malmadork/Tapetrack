<div class="mobile-header header-with-icon header">
    <div class="mobile-header-inner">
        <div class="arrow-icon disabled"><a href="" class="referrer"><i class="bi bi-arrow-left-short"></i></a></div>
        {{#if logo}}
        <a href="/"><img src="/images/tapetrack-160x160.png" srcset="/images/tapetrack-160x160.svg" class="nav-icon"></a>
        {{else}}
        <h1 class="left-align title">{{title}}</h1>
        {{/if}}
    </div>
    {{#if profile}}
    <div class="header-end settings-wheel">
      <a href="/settings"><i class="bi bi-gear-fill"></i></a>
    </div>
    {{else}}
    <div id="buttons">
        <a class="btn btn-1 logout-nav">Log Out</a>
    </div>
    {{/if}}
</div>